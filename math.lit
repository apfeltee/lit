
var testtbl = [
["0x777 & 0xFF", 119],
["1234 & 5678", 1026],
["12 << 48", 786432],
["233 >> 2", 58],
["12 | 48", 60],
["55 ^ 48", 7]
];

for(var i=0; i<testtbl.length; i++)
{
    var t = testtbl[i];
    var src = t[0];
    var exp = t[1];
    var rt = eval(src);
    var pre = "OK  ";
    if(rt != exp)
    {
        pre = "FAIL";
    }
    println("", pre, ": (", src, ") = ", rt, " (", exp, ")");
}


